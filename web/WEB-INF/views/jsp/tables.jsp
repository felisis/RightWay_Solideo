                                    <th>제목</th>
                                    <th>작성자</th>
                                    <th>작성날짜</th>
                                    <th>조회수</th>
                                </tr>
                                </thead>
                                <tfoot>
                                <tr>
                                    <th>num</th>
                                    <th>title</th>
                                    <th>writer</th>
                                    <th>refdate</th>
                                    <th>count</th>
                                </tr>
                                </tfoot>
                                <tbody>
                                <c:forEach items="${BoardList}" var="board">
                                    <tr>
                                        <td>${board.num}</td>
                                        <td><a href ="move_update.do?num=${board.num}">${board.title}</a></td>
                                        <td>${board.writer}</td>
                                        <td>${board.refdate}</td>
                                        <td>${board.count}</td>
                                    </tr>
                                </c:forEach>
                                </tbody>
                            </table>
